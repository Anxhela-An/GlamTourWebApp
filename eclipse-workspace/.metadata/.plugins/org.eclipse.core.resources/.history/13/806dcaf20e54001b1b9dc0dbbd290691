import java.util.*;
public class Vektoret
{   static int[] notat = new int[3];
static String [] Studente = new String [3];
    
	public static int mbushNotat()
	{ Scanner sc = new Scanner(System.in);
		
	  for(int i=0; i<notat.length;i++)
	{ System.out.println("Fut notat ne vektor: "+ (i+1));
      notat[i]=sc.nextInt();	
	}
	 return mbushNotat();
	}
   public static String mbushStudente(String []s)
   {
	   Scanner sc= new Scanner(System.in);
	   for(int j=0; j<s.length;j++)
	   {   System.out.println("Fut studentet: ");
		   s[j]=sc.nextLine();
	   }
	   return mbushStudente(s);
   }
   
 public void notaMes(int []n)
 {
	 int count = 0;
	 int sum=0;
	 for(int k=0; k<n.length;k++)
	 {
		 sum=sum+n[k];
		 count=count+1;
	 }
	 double mesatare= sum/count;
	 int numero=0;
	 for(int i=0; i<n.length;i++)
	 {
		 if(mesatare>n[i])
		 { numero++;
			 
		 }
	 }
	 System.out.println("Mesatarja eshte: "+mesatare);
	 System.out.println("Numri i studenteve me note nen mesatare: "+ numero);
 }
 
public static void main(String []args)
{	
	int rezultati_notat=mbushNotat();
	String rezultati_studentet=mbushStudente(Studente);
}

}
