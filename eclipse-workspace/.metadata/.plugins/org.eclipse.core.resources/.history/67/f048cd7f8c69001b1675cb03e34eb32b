public class Udhetim {

    private String destinacion;

    private String nisja;

    private Double cmimi;

    public Udhetim( String nisja, String destinacion, Double cmimi) {
        this.destinacion = destinacion;
        this.nisja = nisja;
        this.cmimi = cmimi;
    }

    public Double getCmimi() {
        return cmimi;
    }

    public String getDestinacion() {
        return destinacion;
    }

    public String getNisja() {
        return nisja;
    }
}

