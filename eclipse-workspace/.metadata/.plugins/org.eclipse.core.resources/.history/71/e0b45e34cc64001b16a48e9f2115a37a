
public class Punonjes 
{ private String numri;
  private String emri;
  
  public Punonjes(String numri,String emri)
  {
	  this.numri=numri;
	  this.emri=emri;
  }
  void setEmri(String emri)
  { emri=this.emri;
	  
  }
  
  String getEmri()
  {
	  return emri;
  }
  
  void setNumri(String numri)
  {
	numri=this.numri;
  }
  
  String getNumri()
  {
	  return numri;
  }

}
